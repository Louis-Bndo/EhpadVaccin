@model IEnumerable<EhpadVaccin.ORM.Injection>

@{ ViewData["Title"] = "ListeInjections"; }

<h1>Liste des injections</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.personne)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dateInjection)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dateRappel)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.numLot)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.typeVaccin)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.marque)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.personne)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.dateInjection)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.dateRappel)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.numLot)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.typeVaccin)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.marque)
    </td>
</tr>
}
    </tbody>
</table>
